#include <string.h>
#include <stdlib.h>
//No1
int main()
{       
        char a[20];
        char b[20];
        int c,j=0;
        scanf("%s",a);
        getchar();
        c=strlen(a);
        /*for(int i=c-1;i<0;i--){
                b[j]=a[i];
                j++;
        }*/
        b[0]=a[c-1];
        b[1]=a[3];
        b[2]=a[2];
        b[3]=a[1];
        b[c-1]=a[0];
        printf("%s",b);
        return 0;
}
